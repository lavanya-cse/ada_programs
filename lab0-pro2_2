#include<stdio.h>

int compute()
{
        int arr[100],size,i,key;
        scanf("%d",&size);
        scanf("%d",&key);
        for(i=0;i<size;i++)
        {
            scanf("%d",&arr[i]);
        }
        for(i=0;i<size;i++)
        {
            if(arr[i]==key)
            {
                return 1;
            }
        }
        return -1;
}

int main()
{
    int testcase,i,temp;
    scanf("%d",&testcase);
    for(i=0;i<testcase;i++)
        {
            temp=compute();
            printf("%d \n",temp);
        }
}
