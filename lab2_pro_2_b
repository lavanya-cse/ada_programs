#include <iostream>
using namespace std;

int bubble_sort(int a[],int n,int k);

int main()
{
	int a[100],i,k,n;
	cout<<"enter the size\n";
	cin>>n;
	cout<<"enter the element\n";
	for(i=0;i<n;i++)
		cin>>a[i];
	cout<<"enter the kth\n";
	cin>>k;
	bubble_sort(a,n,k);
	cout<<"sorted list\n";
	for(i=0;i<n;i++)
	cout<<a[i]<<"\n";
	cout<<"kth largest element\n";
	for(i=n-1;i>k;i--)
	cout<<a[i]<<"\n";
	return 0;
}


int bubble_sort(int a[],int n,int k)
{
	int temp,i,j;
	for(i=0;i<k-1;i++)
	{
		for(j=0;j<n-2-i;j++)
		{
			if(a[j+1]<a[j])
			{
				temp=a[j+1];
				a[j+1]=a[j];
				a[j]=temp;
			}
		}
	}
}
