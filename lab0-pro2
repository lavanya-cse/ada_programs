#include <stdio.h>

int main()
{
	int arr[100],k,size,i;
	printf("enter the size of an array\n");
	scanf("%d",&size);
	printf("enter the elements of an array\n");
	for(i=0;i<size;i++)
	{
		scanf("%d",&arr[i]);
	}
	printf("enter the key element to search");
	scanf("%d",&k);
	for(i=0;i<size;i++)
	{
		if(arr[i]==k)
			{
				printf("1 \n");
				return 0;
			}
	}
	printf("-1 \n");
	return 0;
	
}
