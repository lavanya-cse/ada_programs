#include <stdio.h>

int main()
{
	int arr[20],i,size,max;
	printf("enter the size of an array\n");
	scanf("%d",&size);
	printf("enter the element of an array\n");
	for(i=0;i<size;i++)
	{
		scanf("%d",&arr[i]);
	}
	max=arr[0];
	for(i=0;i<size;i++)
	{
		if(max<arr[i])
			max=arr[i];
	}
	printf("the maximum num is %d \n",max);
	
}
